{"version": 1,
 "formatters": {"brief": {"format": "%(levelname)-8s: %(name)-15s: %(message)s"},
                "full": {"format": "%(asctime)s %(name)-15s %(levelname)-8s %(message)s"}
                },
 "handlers": {"filelog": {
                   "class": "logging.FileHandler",
                   "formatter": "full",
                   "level": "WARN",
                   "filename": "log/planner.log"}
              },
 "root": {"level": "WARN", "handlers": ["filelog"]}}